<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{{ app_name|default("d2s") }}</title>
        {% if analytics is defined and analytics %}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
            ga('create', '{{ analytics }}', 'auto');
            ga('send', 'pageview');
        </script>
        {% endif %}
        {% block javascripts %}
        <script src="//code.jquery.com/jquery-2.2.0.min.js"></script>
        <!-- Begin Cookie Consent plugin by Silktide - http://silktide.com/cookieconsent -->
        <script type="text/javascript">
            window.cookieconsent_options = {"message":"Tento web využívá cookies. Jeho používáním s tím vyjadřujete souhlas.","dismiss":"Rozumím!","learnMore":"Více o cookies","link":"{{ path('net_tomaskadlec_d2s_terms_index') }}#cookies","theme":"dark-top"};
        </script>
        <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/1.0.9/cookieconsent.min.js"></script>
        <!-- End Cookie Consent plugin -->
        {% endblock %}
        {% block stylesheets %}
        <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700italic,700,800,800italic&subset=latin,latin-ext' rel='stylesheet' type='text/css'>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="{{ asset('bundles/nettomaskadlecd2s/css/base.css') }}"/>
        <link rel="stylesheet" href="{{ asset('bundles/nettomaskadlecd2s/css/application.css') }}"/>
        <link rel='stylesheet' href="{{ asset('bundles/nettomaskadlecd2s/css/application_hires.css') }}" media='only screen and (min--moz-device-pixel-ratio: 2), only screen and (-o-min-device-pixel-ratio: 2/1), only screen and (-webkit-min-device-pixel-ratio: 2), only screen and (min-device-pixel-ratio: 2)' />
        <meta name="viewport" content="width=device-width, initial-scale=1">
        {% endblock %}
        <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
    </head>
    <body>
        <!--[if lte IE 9]>
        <div class="ielte7" style="background: yellow; margin: 0; border: 0.3em solid red;">
            <dl>
                <dt>Používáte zastaralou verzi prohlížeče!</dt>
                <dd><a href="http://windows.microsoft.com/cs-cz/internet-explorer/download-ie">Aktualizujte</a> na vyšší verzi,</dd>
                <dd>nainstalujte <a href="https://www.google.com/chrome">alternativní prohlížeč</a> nebo</dd>
                <dd>kontaktujte správce svého počítače.</dd>
            </dl>
        </div>
        <![endif]]]-->
        {% block breadcrumbs %}
        <nav class="breadcrumbs">
            {# https://gist.github.com/fsevestre/b378606c4fd23814278a #}
            {{ knp_menu_render('main', {'template': '::breadcrumbs.html.twig'}) }}
        </nav>
        {% endblock %}
        {% block header %}
        <header>
            {% set current_menu_item = knp_menu_get_current_item('main') %}
            <h1>{{ current_menu_item.attribute('title', current_menu_item.label) }}</h1>
        </header>
        {% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}
        <footer>
            {#<a href="https://github.com/tomaskadlec/d2s" class="right"></a>#}
            <div class="container">
                <nav>
                    {{ knp_menu_render('main')  }}
                </nav>
                <nav>
                    {{ knp_menu_render('external', {'template': '::external.html.twig'}) }}
                    {% if app_info.version %}
                        Verze: {{ app_info.version }}
                    {% endif %}
                </nav>
            </div>
        </footer>
        {% endblock %}
    </body>
</html>
